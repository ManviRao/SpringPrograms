<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
   <bean id="spec" class="AutowiringByName.Specification">
	<property name="make" value="Toyota"/>
	<property name="model" value="corolla"/>
	</bean>
	 <bean id="spec1" class="AutowiringByName.Specification">
	<property name="make" value="Toyota 2"/>
	<property name="model" value="corolla 2"/>
	</bean>
	
    <bean id='mycar' class="AutowiringByName.Car" autowire="byName"/>
    
</beans>
